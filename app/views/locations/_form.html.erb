<%= form_for @user.location do |f| %>

  <%= f.label :street_address %>
  <%= f.text_field :street_address %>
  <br>
  <%= f.label :city %>
  <%= f.text_field :city %>
  <br>
  <%= f.label :state %>
  <%= f.text_field :state %>
  <br>
  <%= f.label :country %>
  <%= f.text_field :country %>
  <br><br>
  <%= f.submit %>
<% end %>
