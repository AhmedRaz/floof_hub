<%= form_for setup_user(@user) do |f| %>

  <%= f.label :name %>
  <%= f.text_field :name %>
  <br>
  <%= f.label :age %>
  <%= f.number_field :age %>
  <br>
  <%= f.label :dwelling_type %>
  <%= f.text_field :dwelling_type %>
  <br>
  <%= f.label :household_members %>
  <%= f.number_field :household_members %>
  <br>
  <%= f.label :bio %>
  <%= f.text_area :bio %>
  <br>
  <%= f.label :kids %>
  <%= f.check_box :kids %>

  <br>
  <%= f.label :email_address %>
  <%= f.email_field :email_address %>
  <br>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <br>
  <%= f.label :shelter_employee %>
  <%= f.check_box :shelter_employee %>
  <br>

  <%= render "locationform" %>
  
  <%= f.submit "Register Here"%>
  <br>


<% end %>
